delta DTransferIntraBank;
uses MAccountModel;

modifies class AccountImpl {
    modifies Unit transfer(AccountImpl other, Int amount, Int fee) {
        total = amount + fee;
		if (this.balance < total) {
			println("Transfer fails")
		}
		else {
			this.balance = this.balance - total;
			other.balance = this.balance + total;
		}
    }
}